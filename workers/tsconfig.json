{
  "compilerOptions": {
    "target": "es2021",
    "lib": ["es2021"],
    "module": "esnext",
    "types": ["@cloudflare/workers-types"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipDefaultLibCheck": true
  },
  "include": [
    "src/**/*"
  ]
}
